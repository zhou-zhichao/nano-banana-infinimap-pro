{
  "name": "infinimap",
  "version": "0.1.0",
  "private": true,
  "packageManager": "yarn@4.12.0",
  "scripts": {
    "dev": "node scripts/next-with-shell.cjs dev",
    "dev:web": "node scripts/next-with-shell.cjs dev",
    "dev:py": "./.venv/Scripts/python -m uvicorn pyservice.main:app --host 127.0.0.1 --port 8001 --env-file .env.local",
    "dev:py:reload": "./.venv/Scripts/python -m uvicorn pyservice.main:app --host 127.0.0.1 --port 8001 --reload --env-file .env.local",
    "build": "node scripts/next-with-shell.cjs build",
    "start": "node scripts/next-with-shell.cjs start",
    "lint": "next lint",
    "check:parent-policy": "node scripts/check-parent-policy.cjs",
    "regen:parents": "node scripts/regen-parents.cjs",
    "tools:moon:import": "python tools/import_nasa_moon_tiles.py",
    "tools:moon:import:custom": "python tools/import_nasa_moon_tiles.py"
  },
  "dependencies": {
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@radix-ui/themes": "^3.2.1",
    "leaflet": "^1.9.4",
    "lucide-react": "^0.542.0",
    "next": "^15.5.9",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-leaflet": "^5.0.0",
    "sharp": "^0.34.3",
    "uuid": "^11.1.0",
    "zod": "^4.1.5"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.12",
    "@types/leaflet": "^1.9.20",
    "@types/node": "^24.3.0",
    "@types/react": "^19.1.12",
    "@types/react-dom": "^19.1.9",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.34.0",
    "eslint-config-next": "^15.5.9",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.12",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  }
}
